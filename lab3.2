#include <iostream>
#include <algorithm>
using namespace std;
int rezult(int mas[100],int  n)
{
	sort ( mas ,mas +n);
	if (mas[0]*mas[1] == mas[n-2]* mas[n-3]  )
	{
		cout<< mas[n-1]<< ' ' << mas[0]<< ' ' << mas[1]<< endl;
		cout <<  mas[n-1]<< ' ' << mas[n-2]<< ' ' << mas[n-3]<< endl;
		system ("pause");
		return 0;
	}
	if (mas[0]*mas[1] > mas[n-2]* mas[n-3]  )
	{
		cout << mas[n-1]<< ' ' << mas[0]<< ' ' << mas[1]<< endl;

	} else 
	{
		cout << mas[n-1]<< ' ' << mas[n-2]<< ' ' << mas[n-3]<< endl;
	}
	system("pause");
}
int main()
{
	int mas[100]={0};
	int n;
	cout << "Input n"<< endl;
	cin >> n;
	for (int i=0; i<n; i++)
	{
		cin >> mas[i];
	}
	rezult(mas,n);
}
