#include <iostream>
#include <cstdio>
#include <cstring>
#include <stack>
#include <algorithm>

using namespace std;

int formula(char str[])
{
	int lenth = strlen(str);
	stack <int> symbol, digit;
	for (int i = 0; i < lenth; ++i)
		if (isdigit(str[i]))
			digit.push(str[i] - '0');
		else
			if (str[i] == 'x')
				symbol.push(0);
			else
				if (str[i] == 'n')
					symbol.push(1);
				else
					if (str[i] == ')') 
					{
						int t = digit.top();
						digit.pop();
						if (symbol.top())
							digit.top() = min(digit.top(), t);
						else
							digit.top() = max(digit.top(), t);
						symbol.pop();
					}
	return digit.top();
}

int main()
{
	return 0;
}
